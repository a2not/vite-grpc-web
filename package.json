{
    "name": "vite-project",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "gen:ts-proto": "protoc -I=./proto echo.proto --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=.:./src/ts-proto-gen --ts_proto_opt=outputClientImpl=grpc-web"
    },
    "dependencies": {
        "@improbable-eng/grpc-web": "^0.15.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ts-proto": "^1.166.4"
    },
    "devDependencies": {
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "@vitejs/plugin-react": "^4.2.0",
        "eslint": "^8.53.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.4",
        "typescript": "^5.2.2",
        "vite": "^5.0.0"
    }
}
